@page "{id}"
@model Pages.Todos.EditModel

<h1>Edit todo</h1>

<div>
    <form method="post">
        <div asp-validation-summary="All" class="text-danger"></div>
        @await Html.PartialAsync("_Form", Model.Todo)
        <button type="submit" class="btn btn-primary">Update todo</button>
    </form>
</div>
